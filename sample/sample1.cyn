x = 0
y = 0

10 times do
  move to x, y

  draw rectangle where
    width = 100
    height = 100
    color = red
  end

  x = x + 10
  y = y + 10
end

10 times do
  move to x, y

  draw circle where
    radius = 10
    color = orange
  end
end

move to (50, 50)
draw line where
  color = black
  thickness = 2
  endpoint = (100, 100)
end

